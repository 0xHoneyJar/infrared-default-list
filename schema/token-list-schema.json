{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "tokens": {
      "items": {
        "properties": {
          "address": {
            "pattern": "^0x[a-fA-F0-9]{40}$",
            "type": "string"
          },
          "chainId": { "type": "number" },
          "decimals": { "type": "number" },
          "image": { "type": "string" },
          "name": { "type": "string" },
          "symbol": { "type": "string" },
          "tags": {
            "items": { "type": "string" },
            "type": "array"
          }
        },
        "required": [
          "address",
          "chainId",
          "decimals",
          "image",
          "name",
          "symbol"
        ],
        "type": "object"
      },
      "type": "array"
    }
  },
  "required": ["tokens"],
  "type": "object"
}
